        <elements xsi:type="org.nasdanika.vinci.app:Action" title="Luscious Spark" id="0912961a-f220-4b3b-b4fa-a3a9a8d02704" text="Luscious Spark" linkedElements="ab1f08f2-a749-46ad-9009-5009d8217d15 013f84e0-516d-4637-a274-9b3237041ca2" role="Section" sectionStyle="Card tab">
          <elements xsi:type="org.nasdanika.vinci.app:Action" title="Вкладка с фотографией" id="ab1f08f2-a749-46ad-9009-5009d8217d15" text="" icon="fas fa-camera" role="Section" markdownContent="![Luscious Spark](img/LusciousSpark.jpg &quot;Luscious Spark&quot;)"/>

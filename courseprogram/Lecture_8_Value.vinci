  <elements xsi:type="org.nasdanika.vinci.app:ActionCategory" title="Контент-элемент Value" id="ed3ecf62-b825-4214-8488-487c8921084f" text="Контент-элемент Value" linkedElements="ab6dded9-bdd3-485b-b02b-6eaf3624f25c">
    <appearance background="Dark">
      <attributes xsi:type="org.nasdanika.ncore:Object" name="style">
